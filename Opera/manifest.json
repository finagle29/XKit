{
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "xkit.css" ],
      "exclude_globs": [ "*.media.tumblr.com*", "*tumblr.com/upload/image*" ],
      "js": [ "bridge.js", "jquery.js", "tiptip.js", "moment.js", "nano.js", "xkit.js" ],
      "matches": [ "*://*.tumblr.com/*" ]
   } ],
   "description": "A fork of XKit, the extension framework for Tumblr.",
   "homepage_url": "https://github.com/new-xkit/XKit",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "minimum_opera_version": "31.0",
   "name": "New XKit",
   "permissions": ["*://*/*", "*://*/", "unlimitedStorage", "storage", "*://*.tumblr.com/" ],
   "version": "7.7.1",
   "web_accessible_resources": [ "manifest.json", "editor.js" ],
   "content_security_policy": "script-src 'self'; object-src 'self'; 'unsafe eval'"
}
